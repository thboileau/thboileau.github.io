.preload .bg-hexagons {
  display: none;
}

.bg-hexagons {
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
}

.bg-hexagons li {
  position: absolute;
  list-style: none;
  display: block;
  bottom: -170px;
  -webkit-animation: square 25s infinite;
  animation: square 25s infinite;
  -webkit-transition-timing-function: linear;
  transition-timing-function: linear;
}

.bg-hexagons li svg {
  width: 40px;
  height: auto;
}

.bg-hexagons li svg path {
  fill: rgba(255, 255, 255, 0.35);
}

.bg-hexagons li svg path:first-of-type {
  fill: rgba(255, 255, 255, 0.15);
}

.bg-hexagons li:nth-child(1) {
  left: 10%;
}

.bg-hexagons li:nth-child(2) {
  left: 20%;
  animation-delay: 2s;
  animation-duration: 17s;
}

.bg-hexagons li:nth-child(2) svg {
  width: 80px;
  height: auto;
}

.bg-hexagons li:nth-child(3) {
  left: 25%;
  animation-delay: 4s;
}

.bg-hexagons li:nth-child(4) {
  left: 40%;
  animation-duration: 22s;
}

.bg-hexagons li:nth-child(4) svg {
  width: 60px;
  height: auto;
}

.bg-hexagons li:nth-child(4) svg path {
  fill: rgba(255, 255, 255, 0.45);
}

.bg-hexagons li:nth-child(4) svg path:first-of-type {
  fill: rgba(255, 255, 255, 0.25);
}

.bg-hexagons li:nth-child(5) {
  left: 70%;
}

.bg-hexagons li:nth-child(6) {
  left: 80%;
  animation-delay: 3s;
}

.bg-hexagons li:nth-child(6) svg {
  width: 120px;
  height: auto;
}

.bg-hexagons li:nth-child(6) svg path {
  fill: rgba(255, 255, 255, 0.4);
}

.bg-hexagons li:nth-child(6) svg path:first-of-type {
  fill: rgba(255, 255, 255, 0.2);
}

.bg-hexagons li:nth-child(7) {
  left: 32%;
  animation-delay: 7s;
}

.bg-hexagons li:nth-child(7) svg {
  width: 160px;
  height: auto;
}

.bg-hexagons li:nth-child(8) {
  left: 55%;
  animation-delay: 15s;
  animation-duration: 40s;
}

.bg-hexagons li:nth-child(8) svg {
  width: 20px;
  height: auto;
}

.bg-hexagons li:nth-child(9) {
  left: 25%;
  animation-delay: 2s;
  animation-duration: 40s;
}

.bg-hexagons li:nth-child(9) svg {
  width: 10px;
  height: auto;
}

.bg-hexagons li:nth-child(9) svg path {
  fill: rgba(255, 255, 255, 0.5);
}

.bg-hexagons li:nth-child(9) svg path:first-of-type {
  fill: rgba(255, 255, 255, 0.3);
}

.bg-hexagons li:nth-child(10) {
  left: 90%;
  animation-delay: 11s;
}

.bg-hexagons li:nth-child(10) svg {
  width: 160px;
  height: auto;
}

@-webkit-keyframes square {
  0% {
    transform: translateY(0);
  }
  100% {
    transform: translateY(-700px) rotate(600deg);
  }
}

@keyframes square {
  0% {
    transform: translateY(0);
  }
  100% {
    transform: translateY(-700px) rotate(600deg);
  }
}

