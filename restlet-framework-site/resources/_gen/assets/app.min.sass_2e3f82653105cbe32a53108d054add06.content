.skew-top-grey {
  position: relative;
  z-index: 1;
  width: 100%;
  padding: 0 0 150px;
}

.skew-top-grey:before {
  content: "";
  width: 100%;
  display: block;
  height: 100%;
  position: absolute;
  top: -67px;
  transform: skewY(-4deg);
  background-color: #f8f8f8;
  z-index: -1;
}

.skew-top-white {
  position: relative;
  z-index: 1;
  width: 100%;
  background-color: #FFFFFF;
  padding: 0 0 150px;
}

.skew-top-white:before {
  content: "";
  width: 100%;
  display: block;
  height: 50%;
  position: absolute;
  top: -67px;
  transform: skewY(-4deg);
  background-color: #FFFFFF;
  z-index: -1;
}

.skew-top-blue {
  position: relative;
  z-index: 1;
  width: 100%;
  background-color: #2E465B;
  padding: 0 0 150px;
}

.skew-top-blue:before {
  content: "";
  width: 100%;
  display: block;
  height: 50%;
  position: absolute;
  top: -67px;
  transform: skewY(-4deg);
  background-color: #2E465B;
  z-index: -1;
}

.skew-top-black {
  position: relative;
  z-index: 1;
  width: 100%;
  background-color: #262A30;
  padding: 0;
}

.skew-top-black:before {
  content: "";
  width: 100%;
  display: block;
  height: 50%;
  position: absolute;
  top: -67px;
  transform: skewY(-2deg);
  background-color: #262A30;
  z-index: -1;
}

.m-top-30 {
  margin-top: 30px;
}

.m-top-40 {
  margin-top: 40px;
}

.m-top-50 {
  margin-top: 50px !important;
}

.m-bottom-50 {
  margin-bottom: 50px;
}

.m-bottom-30 {
  margin-bottom: 30px;
}

@media only screen and (min-width: 768px) {
  .eq-height {
    display: flex;
  }
}

.eq-height .flex-item {
  display: flex;
  flex: 1;
}

.eq-height .flex-item > div {
  flex: 1;
}

.center-flex {
  justify-content: center;
}

.flex-wrap {
  flex-wrap: wrap;
}

.abs-hv-center {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.sparky-happy {
  background: url(../images/shared/sparky-happy.svg) no-repeat;
  background-size: contain;
  display: none;
  position: absolute;
  top: -140px;
  left: 0;
  width: 162px;
  height: 197px;
}

@media only screen and (min-width: 768px) {
  .sparky-happy {
    display: inline-block;
  }
}

.orbit-bg {
  background-image: url(../images/shared/sections/orbites.png);
  background-repeat: no-repeat;
  background-position: center;
  background-size: 70%;
}

.content-wrapper {
  width: 100%;
  position: relative;
}

.flex-inline {
  display: flex;
  flex-direction: column;
}

@media only screen and (min-width: 768px) {
  .flex-inline {
    flex-direction: row;
    align-items: flex-start;
  }
}

iframe {
  border: none;
  display: block;
}

#map {
  margin: 0 0 20px;
}

@media only screen and (min-width: 480px) {
  #map {
    margin: 0 0 10px;
  }
}

@media only screen and (min-width: 768px) {
  #map {
    margin: 0;
  }
}

