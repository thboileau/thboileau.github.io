.m-navigation .navigation-trigger {
  background: linear-gradient(to bottom, #FFFFFF 0%, #FFFFFF 20%, transparent 20%, transparent 40%, #FFFFFF 40%, #FFFFFF 60%, transparent 60%, transparent 80%, #FFFFFF 80%, #FFFFFF 100%);
  cursor: pointer;
  display: block;
  float: right;
  height: 18px;
  margin-right: 17px;
  width: 21px;
}

.m-navigation .navigation-trigger.collapse ~ .navigation-list {
  background: #FFFFFF;
  border-radius: 3px;
  display: block;
  padding: 20px 0;
  position: absolute;
  top: 72px;
  left: 0;
  width: 100%;
  text-align: center;
}

.m-navigation .navigation-trigger.collapse ~ .navigation-list:before {
  content: '';
  border-bottom: 10px solid #FFFFFF;
  border-left: 10px solid transparent;
  border-right: 10px solid transparent;
  top: -10px;
  position: absolute;
  right: 7px;
  width: 0;
  height: 0;
  transform: translateX(-50%);
}

.m-navigation .navigation-trigger.collapse ~ .navigation-list li > span,
.m-navigation .navigation-trigger.collapse ~ .navigation-list li > a {
  color: #464B55;
  cursor: pointer;
  display: block;
  font-size: 18px;
  line-height: 48px;
}

